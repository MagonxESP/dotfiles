# Zsh plugins submodules

[submodule "zsh-plugin/zsh-autosuggestions"]
	path = share/config/zsh/custom/plugins/zsh-autosuggestions
	url = https://github.com/zsh-users/zsh-autosuggestions

[submodule "zsh-plugin/zsh-history-substring-search"]
	path = share/config/zsh/custom/plugins/zsh-history-substring-search
	url = https://github.com/zsh-users/zsh-history-substring-search.git

[submodule "zsh-plugin/zsh-syntax-highlighting"]
	path = share/config/zsh/custom/plugins/zsh-syntax-highlighting
	url = https://github.com/zsh-users/zsh-syntax-highlighting.git
